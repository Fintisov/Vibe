        :-moz-placeholder, ::-moz-placeholder {
            color: #121212;
            opacity: 1
        }

        :-ms-input-placeholder {
            color: #121212;
            opacity: 1
        }

        .flex {
            display: -ms-flexbox;
            display: flex
        }

        .hidden {
            display: none !important;
            visibility: hidden
        }

        #skip-to-content {
            position: fixed;
            top: 127px;
            left: 0;
            transform: translateX(-100%)
        }

        img, svg, video {
            width: 100%;
            object-fit: cover
        }

        button {
            background: none;
            border: none;
            padding: 0;
            cursor: pointer
        }

        * {
            box-sizing: border-box
        }

        html, html a {
            -webkit-font-smoothing: antialiased !important;
            -moz-osx-font-smoothing: grayscale
        }

        html, html a {
            overscroll-behavior: none
        }

        html {
            width: 100%
        }

        html.touch {
            overflow-y: auto
        }

        body, html.touch {
            overflow-x: hidden
        }

        body {
            font-size: 16px;
            width: 100%;
            overscroll-behavior: none
        }

        main {
            padding-top: 102px
        }

        @media screen and (max-width: 959px) {
            main {
                padding-top: 92px
            }
        }

        #global-mask {
            background-color: #fafafa;
            z-index: 1001
        }

        #click-mask, #global-mask {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%
        }

        #click-mask {
            pointer-events: all;
            cursor: wait;
            z-index: 101
        }

        .slider {
            width: 100%
        }

        .slider .inner {
            display: -ms-flexbox;
            display: flex;
            width: 100%
        }

        .slider .slides {
            width: 100%;
            overflow: hidden
        }

        .slider .slide {
            position: relative
        }

        .product-slider .slider {
            width: calc(100% + 36px)
        }

        .product-slider .slides {
            padding: 18px
        }

        html.touch .product-slider .slider {
            width: 100%
        }

        html.touch .product-slider .slides {
            padding: 0
        }

        .sr-only {
            position: absolute;
            left: -10000px;
            top: auto;
            width: 1px;
            height: 1px;
            overflow: hidden
        }

        .tab-wrapper {
            position: relative
        }

        .tab-wrapper .tab {
            color: #121212;
            position: absolute;
            visibility: hidden;
            opacity: 0;
            top: 0;
            transition: opacity .3s cubic-bezier(.445, .05, .55, .95), visibility 0s .3s
        }

        .tab-wrapper .tab.active {
            transition: opacity .4s cubic-bezier(.445, .05, .55, .95) .15s, visibility 0s .15s;
            visibility: visible;
            opacity: 1;
            position: relative
        }

        .tab-wrapper.no-fade .tab, .tab-wrapper.no-fade .tab.active {
            transition: none
        }

        @media screen and (max-width: 767px) {
            .pdp-hero .thumbnail-wrapper .ar-wrapper {
                width: 100vw;
                min-width: 100vw
            }
        }

        #header {
            opacity: 0;
            visibility: hidden
        }

        .gradient-ref {
            position: absolute;
            pointer-events: none
        }

        @font-face {
