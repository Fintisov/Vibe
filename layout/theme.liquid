                                            {% continue %}
                                        {% endif %}
                                        <a class="text-m shift-word black" href="{{ link.url }}"><span
                                                    data-replace="{{ link.title }}"><span>{{ link.title }}</span></span></a>
                                    {% endfor %}
                                    <a class="read-more" href="/collections/brands">See
                                        All{% include 'icon-arrow-red' %}</a>
                                </div>
                                <div class="brands-grid">
                                    {% for link in linklists.brands-menu.links %}
                                        {% if link.title contains "|-|" %}
                                            {% assign collectionArr = link.url | split: "/" %}
                                            {% assign thisCollection = collections[collectionArr.last] %}
                                            {% assign productCount = thisCollection.products | size %}
                                            <div class="brand-logo{% if productCount == 0 %} coming-soon{% endif %}">
                                                {% if productCount == 0 %}<span
                                                        class="coming-soon-text">Coming<br>soon</span>
                                                    <div>
                                                        <img class="image preload-global"
                                                             data-preload-desktop="https:{{ thisCollection.featured_image | image_url }}"
                                                             data-preload-mobile="https:{{ thisCollection.featured_image | image_url }}">
                                                    </div>
                                                {% else %}
                                                    <a href="{{ link.url }}">
                                                        <img class="image preload-global"
                                                             data-preload-desktop="https:{{ thisCollection.featured_image | image_url }}"
                                                             data-preload-mobile="https:{{ thisCollection.featured_image | image_url }}">
                                                    </a>
                                                {% endif %}
                                            </div>
                                        {% endif %}
                                    {% endfor %}
