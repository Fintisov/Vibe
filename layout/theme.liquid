                            <div class="left">
                                {% for link in linklists.shop-menu.links %}
                                    <div class="column">
                                        <p class="col-header caps">{{ link.title }}</p>
                                        {% if link.links != empty %}
                                            {% for child_link in link.links %}
                                                <a class="text-m shift-word black" href="{{ child_link.url }}"><span
