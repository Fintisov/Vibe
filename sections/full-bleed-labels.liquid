<img class="bg preload" data-preload-desktop="https:{{ section.settings.image | image_url }}" data-preload-mobile="https:{{ section.settings.image_mobile | image_url }}">
<div class="c-225">
    <div class="top">
        {% include "icon-logo" %}
        {% include "icon-logo-small" %}
        <h2 class="header-l">{{ section.settings.header }}</h2>
        <p class="text">{{ section.settings.subheader }}</p>
    </div>
    <div class="bottom labels">
        {% for block in section.blocks %}
            <div class="label">
                <p class="title">{{ block.settings.title }}</p>
                <img class="image preload"{% if block.settings.image.alt %} alt="{{ block.settings.image.alt }}"{% endif %} data-preload-desktop="https:{{ block.settings.image | image_url }}" data-preload-mobile="https:{{ block.settings.image | image_url }}">
            </div>
        {% endfor %}
    </div>
</div>




{% schema %}
{
"name": "Full Bleed Labels",
"tag": "section",
"class": "full-bleed-labels mw",
"settings": [
{
"id": "image",
"type": "image_picker",
"label": "Background Image"
},
{
"id": "image_mobile",
"type": "image_picker",
"label": "Background Image Mobile"
},
{
"id": "header",
"type": "text",
"label": "Header"
},
{
"id": "subheader",
"type": "text",
"label": "Sub Header"
}
],
"blocks": [
{
"name": "Label",
"type": "Tile",
"limit": 4,
"settings": [
{
"type": "text",
"id": "title",
"label": "Title"
},
{
"type": "image_picker",
"id": "image",
"label": "Image"
}
]
}
]
}
{% endschema %}