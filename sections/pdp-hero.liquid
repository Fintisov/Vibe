{% assign vendorHandle = product.vendor | remove: "'" | replace: ' ', '-' | downcase %}
<div class="breadcrumbs c-50">
    <a href="/collections/all">Shop</a>
    {% assign vendorHandle = product.vendor | remove: "'" | replace: ' ', '-' | downcase %}
    {% if collections[vendorHandle] != empty %}
        <span>/</span>
        <a href="/collections/{{ vendorHandle }}">{{ vendorHandle| replace: '-', ' ' }}</a>
    {% endif %}
    <span>/</span>
    <p>{{ product.title }}</p>
</div>
<div class="product c-90">
    <div class="left">
        {% assign imageCount = product.images | size %}
        <div class="{% if imageCount > 1 %}slider {% endif %}thumbnail-wrapper mw{% if imageCount == 1 %} mobile-only{% endif %}" data-at="767">
            <div class="slides">
                <div class="inner mw">
                    {% for image in product.images limit: 6 %}
                        <div class="ar-wrapper{% if forloop.first %} active{% endif %}" style="--ar: 100%; --mar: 100%;">
                            <img class="image-mobile preload{% if forloop.first %}-critical main-mobile{% endif %}" data-preload-desktop="https:{{ image | image_url }}" data-preload-mobile="https:{{ image | image_url }}">
                        </div>
                    {% endfor %}
                </div>
            </div>
            {% if imageCount > 1 %}
                <div class="dots mobile-only">
                    {% for image in product.images %}
                        <button class="dot{% if forloop.first %} active{% endif %}"></button>
                    {% endfor %}
                </div>
            {% endif %}
        </div>
        <div class="image-wrapper main desktop-only">
            <img class="preload-critical" data-preload-desktop="https:{{ product.images[0] | image_url }}">
        </div>
    </div>
    <div class="right">
        <div class="vendor-eyebrow">
            {% if product.type != blank %}
                <p class="caps">{{ product.type }}</p>
                <svg width="7" height="7" viewBox="0 0 7 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <circle cx="3.5" cy="3.5" r="3.5" fill="#383838"/>
                </svg>
            {% endif %}
            <p class="caps">{{ product.vendor }}</p>
        </div>
        <div class="header-wrapper">
            <h1 class="header-l">{{ product.title }}</h1>
        </div>
        <div class="review-overview hero-overview" data-id="{{ product.id }}">
            {% render "icon-star" %}
            {% render "icon-star" %}
            {% render "icon-star" %}
            {% render "icon-star" %}
            {% render "icon-star" %}
            <p>(<span class="review-count"></span>) Reviews</p>
        </div>
        <div class="price-row">
            <p class="price">{{ product.first_available_variant.price | money }}</p>
            <p class="compare-at-price with-strike{% if product.first_available_variant.compare_at_price %} visible{% endif %}">{% if product.first_available_variant.compare_at_price %}{{ product.first_available_variant.compare_at_price | money }}{% else %}$0.00{% endif %}</p>
        </div>

        {% if product.metafields.accentuate.additional_product_info != blank %}
            <div class="additional-product-info"> {{ product.metafields.accentuate.additional_product_info }}</div>
        {% endif %}

        {% assign optionsSize = product.options | size %}
        {% if product.has_only_default_variant == false %}
            <div class="all-options">
                {% for option in product.options_with_values %}
                    {% assign valueSize = option.values | size %}
                    {% if option.name != "Title" %}
                        <div class="option-wrapper">
                            <p>{{ option.name }}</p>
                            <div class="drawer replace-label">
                                <button class="label">
                                    <span class="current-label">{{ option.values[0] }}</span>
                                    <svg width="11" height="7" viewBox="0 0 11 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M11 1.40446L5.5 6.5L-2.22733e-07 1.40446L0.97625 0.5L5.5 4.69108L10.0238 0.5L11 1.40446Z" fill="#848484"/>
                                    </svg>
                                </button>
                                <div class="drawer-items select-one">
                                    {% for value in option.values %}
                                        {% if forloop.first %}
                                            <p class="non-color-option button option copy active" data-value="{{ value }}">{{ value }}</p>
                                        {% else %}
                                            <p class="non-color-option button option copy" data-value="{{ value }}">{{ value }}</p>
                                        {% endif %}
                                    {%- endfor %}
                                </div>
                            </div>
                        </div>
                    {% endif %}
                    {% if forloop.first %}
                        <div class="option-wrapper quantity">
                            <p>Quantity</p>
                            <div class="drawer replace-label">
                                <button class="label">
                                    <span class="current-label">1</span>
                                    <svg width="11" height="7" viewBox="0 0 11 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M11 1.40446L5.5 6.5L-2.22733e-07 1.40446L0.97625 0.5L5.5 4.69108L10.0238 0.5L11 1.40446Z" fill="#848484"/>
                                    </svg>
                                </button>
                                <div class="drawer-items select-one">
                                    <p class="non-color-option button copy active" data-value="1">1</p>
                                    <p class="non-color-option button copy active" data-value="2">2</p>
                                    <p class="non-color-option button copy active" data-value="3">3</p>
                                    <p class="non-color-option button copy active" data-value="4">4</p>
                                    <p class="non-color-option button copy active" data-value="5">5</p>
                                </div>
                            </div>
                        </div>
                    {% endif %}
                {%- endfor %}
            </div>
        {% else %}
        <div class="all-options">
            <div class="option-wrapper quantity">
                <p>Quantity</p>
                <div class="drawer replace-label">
                    <button class="label">
                        <span class="current-label">1</span>
                        <svg width="11" height="7" viewBox="0 0 11 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M11 1.40446L5.5 6.5L-2.22733e-07 1.40446L0.97625 0.5L5.5 4.69108L10.0238 0.5L11 1.40446Z" fill="#848484"/>
                        </svg>
                    </button>
                    <div class="drawer-items select-one">
                        <p class="non-color-option button copy active" data-value="1">1</p>
                        <p class="non-color-option button copy active" data-value="2">2</p>
                        <p class="non-color-option button copy active" data-value="3">3</p>
                        <p class="non-color-option button copy active" data-value="4">4</p>
                        <p class="non-color-option button copy active" data-value="5">5</p>
                    </div>
                </div>
            </div>
        </div>
        {%- endif %}

        <form class="atc-form" action="/cart/add">
            <div class="hidden">
                <select name="id">
                    {% for variant in product.variants %}
                        <option value="{{ variant.id }}"
                                {%- if variant == current_variant or product.has_only_default_variant %} selected="selected" {%- endif -%}
                        >
                            {{ variant.title }}  {%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %}
                        </option>
                    {% endfor %}
                </select>
            </div>
            <div class="subscription-wrapper text-s">
                {% render 'skio-plan-picker' %}
            </div>
            {% if product.has_only_default_variant %}
                <button class="button add-to-cart header-m{% if product.available %}{% else %} disabled{% endif %}" type="submit" data-id="{{ product.variants[0].id }}">Add To Cart</button>
            {% else %}
                <button data-variants="{% for variant in product.variants %}{% if forloop.last == true %}{{ variant.id }}||{{ variant.title }}||{{ variant.price }}||{% if variant.compare_at_price != blank %}{{ variant.compare_at_price }}{% else %}false{% endif %}||{% if variant.image != blank %}{{ variant.image |  image_url }}{% else %}false{% endif %}||{{ variant.available }}{% else %}{{ variant.id }}||{{ variant.title }}||{{ variant.price }}||{% if variant.compare_at_price != blank %}{{ variant.compare_at_price }}{% else %}false{% endif %}||{% if variant.image != blank %}{{ variant.image |  image_url }}{% else %}false{% endif %}||{{ variant.available }}|||{% endif %}{% endfor %}" class="button add-to-cart header-m{% if product.available %}{% else %} disabled{% endif %}" type="submit" data-id="{{ product.first_available_variant.id }}">Add To Cart</button>
            {% endif %}
        </form>
    </div>
</div>

{% unless product.metafields.accentuate.hide_iconrow %}
    {% if template.suffix == 'custom' %}
        <div class="c-120 icon-row">
            <div class="wrap">
                <div class="icon-wrap">
